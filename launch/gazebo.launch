<?xml version="1.0"?>
<launch>
    <arg name="package" default="multi_drone_inspection"/>
    <arg name="package_path" default="$(find multi_drone_inspection)"/>

    <node pkg="$(arg package)" type="make_px4_gazebo.sh" name="make_px4_gazebo" />
    <include file="$(find mavros)/launch/px4.launch">
        <arg name="fcu_url" default="udp://:14540@127.0.0.1:14557"/>
    </include>
</launch>